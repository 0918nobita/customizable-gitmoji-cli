{
  "package.json": [
    "prettier-package-json --write",
    "prettier --write",
    "git add"
  ],
  "*[!package].json": ["prettier --write", "git add"],
  ".all-contributorsrc": [
    "all-contributors generate",
    "git add",
    "git add README.md"
  ],
  "*.md": ["prettier --write", "git add"],
  "*.{js,ts}": ["eslint --fix", "prettier --write", "git add"],
  "*.yml": ["prettier --write", "git add"]
}
