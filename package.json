    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.8",
    "jest": "^24.0.0",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
